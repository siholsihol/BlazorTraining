@inherits R_Page
@attribute [R_Page(Title = "Selection")]

<VerticalTab Tabs="tabs" />

@code {
    private Dictionary<string, Type> tabs = new Dictionary<string, Type>();

    protected override Task R_Init_From_Master(object poParameter)
    {
        tabs.Add("R_CheckBox", typeof(CheckBoxMain));
        tabs.Add("R_ComboBox", typeof(ComboBoxMain));
        tabs.Add("R_ComboBoxMultiSelect", typeof(ComboBoxMultiSelectMain));
        tabs.Add("R_DropDownList", typeof(DropDownListMain));
        tabs.Add("R_RadioGroup", typeof(RadioGroupMain));

        return Task.CompletedTask;
    }
}