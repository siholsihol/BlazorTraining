@inherits R_Page

<R_StackLayout>
    <R_StackLayout Row>
        <R_ItemLayout col="4">
            Enabled
        </R_ItemLayout>
        <R_ItemLayout col="8">
            <R_TextBox @bind-Value="@StringValue"
                       Enabled />
        </R_ItemLayout>
    </R_StackLayout>

    <R_StackLayout Row>
        <R_ItemLayout col="4">
            Disabled
        </R_ItemLayout>
        <R_ItemLayout col="8">
            <R_TextBox @bind-Value="@StringValue"
                       Enabled="false" />
        </R_ItemLayout>
    </R_StackLayout>
</R_StackLayout>

@code {
    private string StringValue { get; set; }
}
